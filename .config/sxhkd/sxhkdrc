super + shift + d
	firemenu

super + shift + f
	dwmc togglefloating

super + shift + l
	slock

super + shift + p
	poweroff

super + shift + r
	restart

super + {c,v,n}
	st -e {cmus,cmixer,note}

super + F2
	slock

super + F3
	notify-send "$(apm | sed 's/%.*//' | sed '/[0-9]/ s/$/%/' | head -n 2)"

super + F5
	video